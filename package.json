{
  "name": "codefr-online-ide",
  "version": "1.0.0",
  "description": "Online IDE for the CodeFr programming language",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
